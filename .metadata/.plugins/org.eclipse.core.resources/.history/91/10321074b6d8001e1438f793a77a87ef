package com.application.EmployeeManagementSystem.domain;

import java.time.Instant;

import jakarta.persistence.Entity;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Entity
@Data
@NoArgsConstructor
@AllArgsConstructor
public class Employee {
	Integer id;
	String firstName;
	String lastName;
	Integer age;
	String email;
	Long salary;
	String designation;
	Instant joiningDate;
}
